{
	"name": "pwa-weather",
	"version": "2.0.0",
	"license": "MIT",
	"scripts": {
		"start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
		"build":
			"preact build --template src/assets/template.html --no-prerender --service-worker false && node sw-generator.js",
		"serve":
			"preact build --template src/assets/template.html --no-prerender --service-worker false && preact serve",
		"dev": "preact watch --template src/assets/template.html",
		"lint": "eslint src"
	},
	"browserslist": ["last 1 versions"],
	"eslintConfig": {
		"extends": "eslint-config-synacor"
	},
	"eslintIgnore": ["build/*"],
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-preset-babili": "^0.1.4",
		"babel-preset-env": "^1.6.1",
		"babel-preset-minify": "^0.2.0",
		"babili-webpack-plugin": "^0.1.2",
		"eslint": "^4.5.0",
		"eslint-config-synacor": "^1.1.0",
		"if-env": "^1.0.0",
		"lodash": "^4.17.4",
		"node-sass": "^4.6.0",
		"preact-cli": "^2.0.2",
		"sass-loader": "^6.0.6"
	},
	"dependencies": {
		"idb-keyval": "^2.3.0",
		"preact": "^8.2.1",
		"preact-compat": "^3.17.0",
		"preact-router": "^2.5.5"
	}
}
